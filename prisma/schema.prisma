generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Customers {
  id    Int     @id @default(autoincrement())
  name  String
  lastname String
  email String @unique
  password String 
  address String?
  phone String?
  admin Boolean
}

model Products {
   id Int @id @default(autoincrement())
   name String
   image String
   width Float
   heigth Float
   length Float
   stock Int
}

